<%- include('../partial/client-head.ejs') %>


<body>
<div class="container-fluid">
    <%- include('../partial/client-header.ejs') %>

    <h2 class="text-center mb-3 mt-5 title-content">Products</h2>
    <div class="row">
        <div class="col-lg-2 menu-product"></div>
        <div class="col-lg">
            <div class="col-lg toolbar-product">

            </div>
            <div class="col-lg list-products">

            </div>

        </div>
    </div>
    <div class="tab-content" id="pills-tabContent">
        <div class="tab-pane fade show active latest" id="pills-home" role="tabpanel"
             aria-labelledby="featured-products">
            <div class="row ">
                <% for (var i = 0 ; i < list.length; i++) {
                %>
                <div class="col-lg latest-border ">
                    <div class="latest-top ">
                        <img src="<%= list[i].thumbnail %>" alt="">
                        <div class="latest-icon">
                            <a href="" class="fa fa-shopping-cart btn btn-shop mb-2"></a>
                            <a href="" class="fa fa-heart btn btn-shop mb-2"></a>
                            <a href="" class="fa fa-eye btn btn-shop mb-2"></a>
                        </div>
                    </div>
                    <div class="latest-bot text-center">
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <h3 class="m-3">Beautiful Art Decor Light</h3>
                        <h5>$500.00</h5>
                    </div>
                </div>

                <% }
                %>

            </div>


        </div>

    </div>



    <%- include('../partial/client-Footer.ejs') %>
</div>


</body>


<%- include('../partial/client-script') %>