<%- include('../partial/head-admin.ejs')%>
<body>
<%-include('../partial/left-menu.ejs')%>
<%-include('../partial/header.ejs')%>


<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">Detail Contact</h4>
                    </div>
                    <div class="card-body">
                        <form action="#">
                            <div class="row">
                                <div class="col-md-6 ">
                                    <div class="form-group">
                                        <label>Name Product</label>
                                        <div>
                                            <%= contact.name %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Email</label>
                                        <div>
                                            <%= contact.email%>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Subject</label>
                                        <div>
                                            <%= contact.subject %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Subject</label>
                                        <div>
                                            <%= contact.messeage %>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="clearfix"></div>
                        </form>
                        <div class="row">
                            <div class="card-footer">
                                <div class="form-group">
                                    <a href="/admin/product/contact"><button class="poniter-help btn btn-info btn-fill">Back to list</button></a>
                                    <a href="/admin/product/delete-contact?id=<%= contact._id%>"><button class="poniter-help btn btn-danger btn-fill">Delete</button></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<%-include('../partial/footer.ejs')%>



<%- include('../partial/admin-script.ejs')%>
</body>

</html>